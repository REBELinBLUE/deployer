{
  "private": true,
  "name": "deployer-frontend",
  "version": "0.0.1",
  "description": "The frontend source for Deployer",
  "author": "Stephen Ball",
  "license": "MIT",
  "homepage": "https://github.com/REBELinBLUE/deployer-frontend",
  "dependencies": {
    "admin-lte": "^2.4.3",
    "backbone": "^1.3.3",
    "bootstrap": "^3.3.7",
    "bootstrap-3-typeahead": "^4.0.2",
    "bootstrap-tagsinput": "^0.7.1",
    "brace": "^0.10.0",
    "cropper": "^3.1.6",
    "font-awesome": "^4.7.0",
    "html5shiv": "^3.7.3",
    "ionicons": "^3.0.0",
    "jquery": "~2.2.4",
    "jquery-sortable": "^0.9.13",
    "moment": "^2.22.1",
    "respond.js": "^1.4.2",
    "select2": "^4.0.5",
    "socket.io-client": "^2.1.1",
    "toastr": "^2.1.4",
    "underscore": "^1.9.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-istanbul": "^0.12.2",
    "babel-preset-stage-1": "^6.24.1",
    "chai": "^4.1.2",
    "cross-env": "^5.1.6",
    "dirty-chai": "^2.0.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.12.0",
    "laravel-mix": "^1.7.2",
    "mocha": "^4.1.0",
    "rimraf": "^2.6.2",
    "sinon": "^4.5.0",
    "sinon-chai": "^2.14.0",
    "webpack-shell-plugin": "^0.5.0"
  },
  "scripts": {
    "dev": "npm run development",
    "development": "rimraf build/ && cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --watch --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch-poll": "npm run watch -- --watch-poll",
    "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod": "npm run production",
    "production": "rimraf build/ && cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "test": "cross-env NODE_ENV=test mocha",
    "test:coverage": "cross-env NODE_ENV=test babel-node -- ./node_modules/.bin/babel-istanbul cover --include-all-sources --root src/ ./node_modules/mocha/bin/_mocha",
    "lint": "eslint src/"
  }
}
